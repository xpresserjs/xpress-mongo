    "@types/mongodb": "^3.6.12",
    "@types/node": "^15.3.0",
    "@types/uuid": "^8.3.0",
    "deep-object-diff": "^1.1.0",
    "joi": "^17.4.0",
    "mongodb": "3.6.6",
    "object-collection": "^2.0.0",
    "uuid": "^8.3.2"
  },
