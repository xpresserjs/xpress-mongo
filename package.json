{
  "name": "xpress-mongo",
  "version": "1.5.3",
  "description": "Light Weight ODM for mongoDb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npx tsc",
    "play": "ts-node-dev --respawn tests/index.ts",
