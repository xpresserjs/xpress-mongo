  "dependencies": {
    "@types/mongodb": "^3.6.20",
    "deep-object-diff": "^1.1.0",
    "joi": "^17.5.0",
    "mongodb": "3.6.10",
    "object-collection": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/node": "^17.0.5",
    "@types/uuid": "^8.3.3",
    "chance": "^1.1.8",
    "japa": "^4.0.0",
    "nodemon": "^2.0.15",
